      operation 212482 "Animation"
	cpp_inline public explicit_return_type ""
	nparams 4
	  param in name "x" explicit_type "int"
	  param in name "y" explicit_type "int"
	  param in name "width" explicit_type "int"
	  param in name "height" explicit_type "int"
	cpp_decl "    ${comment}${inline}${name}${(}${t0} ${p0}${v0}, ${t1} ${p1}${v1}, ${t2} ${p2}${v2}, ${t3} ${p3}${v3}${)}${volatile}${throw}${abstract} {
            texture.loadFromFile( \"Hero2.png\" );
            blender.loadFromFile( \"Blender.png\" );
            door.loadFromFile( \"BadDoor.png\" );
            key.loadFromFile( \"Key.png\" );
            theclock.loadFromFile( \"Clock.png\" );			

            for (int i = 0; i < nFrames; i++){
                frames[i] = {x + i * width, y, width, height};
            }
        };
"
	
	
	
	
      end

